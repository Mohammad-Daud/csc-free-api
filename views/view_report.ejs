<!DOCTYPE html>
<html>

<head>
    <%- include('partials/meta'); -%>
    <title>

        <%= process.env.APP_NAME %> -
        <%= title %>

    </title>
    <%- include('partials/include_styles'); -%>
</head>

<body>

    <%- include('partials/nav'); -%>

    <div class="container">



        <div class="row">
            <div class="col-lg-12">
                <h2>Report</h2>

                <%- include('partials/notifications'); -%>

                
                


                <table class="table table-bordered table-hover">
                    <thead>
                        <tr>
                                
                            <% userRepCols.forEach(function(userRepCol){ %>
                            <th><%- userRepCol.columnTitle %></th>
                            
                            <% }); %>
                        </tr>
                    </thead>
                    <tbody>
                        <% queryResults.forEach(function(queryResult){ %>
                        <tr>
                        <% userRepCols.forEach(function(userRepCol){ %>
                            <% var prop = userRepCol.columnTitle; %>
                            <td>
                                   
                                <%- queryResult[prop] %>
                            </td>
                        <% }); %>
                        </tr>
                        <% }); %>

                        
                    </tbody>
                </table>





            </div>
        </div>


    </div>









    <%- include('partials/include_js'); -%>




</body>

</html>